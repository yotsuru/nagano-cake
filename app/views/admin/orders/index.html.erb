<h2>注文履歴一覧</h2>

<table>
		  <thead>
		    <tr>
		      <th>注文日</th>
	      	  <th>配送先</th>
		      <th>注文商品</th>
		      <th>支払い金額</th>
		      <th>注文詳細</th>
		    </tr>
		  </thead>
		  
		  <tbody>
		    <% @orders.each do |order| %>
		  　 <tr>
		        <td><%= order.created_at %></td>
		        <td>〒<%= order.postal_code %><br><%= order.address %><br><%= order.name %></td>
		        <td><%= order.item %></td>
    	        <td></td>
     	      </tr>
		    <% end %>
		  </tbody>
		</table>
		
		<%= paginate(@orders) %>